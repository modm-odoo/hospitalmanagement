<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="seq_case_no" model="ir.sequence">
        <field name="name">Hospital Patient</field>
        <field name="code">hospital.patient</field>
        <field name="prefix">HP</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>
    <record id="seq_bill_no" model="ir.sequence">
        <field name="name">Hospital Bill</field>
        <field name="code">hospital.bill</field>
        <field name="prefix">BN</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>
</odoo>